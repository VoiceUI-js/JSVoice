{
  "name": "jsvoice",
  "version": "1.0.2",
  "description": "Modern JavaScript voice command library with speech recognition, synthesis, pattern matching, wake word detection, and real-time audio visualization. Zero dependencies, TypeScript ready, perfect for accessibility and voice-first UIs.",
  "main": "dist/voice-ui.cjs.js",
  "module": "dist/voice-ui.esm.js",
  "unpkg": "dist/voice-ui.umd.min.js",
  "browser": "dist/voice-ui.umd.js",
  "types": "src/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./dist/voice-ui.cjs.js",
      "types": "./src/index.d.ts"
    },
    "./core": {
      "import": "./src/JSVoice.js",
      "types": "./src/index.d.ts"
    },
    "./engines/native": {
      "import": "./src/engines/NativeSpeechEngine.js",
      "types": "./src/index.d.ts"
    },
    "./engines/whisper": {
      "import": "./src/engines/WhisperSpeechEngine.js",
      "types": "./src/index.d.ts"
    },
    "./plugins/visualizer": {
      "import": "./src/modules/AudioVisualizer.js",
      "types": "./src/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\" \"examples/**/*.{html,js}\"",
    "format:check": "prettier --check \"src/**/*.js\" \"test/**/*.js\"",
    "prepare": "npm run build"
  },
  "files": [
    "dist/",
    "src/",
    "examples/",
    "DETAILED_DOCUMENTATION.md",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "voice",
    "voice-commands",
    "voice-control",
    "voice-recognition",
    "speech",
    "speech-recognition",
    "speech-synthesis",
    "text-to-speech",
    "tts",
    "stt",
    "web-speech-api",
    "voice-ui",
    "voice-interface",
    "accessibility",
    "a11y",
    "hands-free",
    "wake-word",
    "pattern-matching",
    "audio-visualization",
    "waveform",
    "amplitude",
    "javascript",
    "typescript",
    "browser",
    "web",
    "frontend",
    "ui",
    "ux",
    "pwa",
    "progressive-web-app",
    "zero-dependencies",
    "lightweight"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VoiceUI-js/VoiceUI.git"
  },
  "homepage": "https://github.com/VoiceUI-js/VoiceUI#readme",
  "bugs": {
    "url": "https://github.com/VoiceUI-js/VoiceUI/issues"
  },
  "author": "JSVoice Contributors",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.28.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^30.1.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.7.4",
    "rollup": "^4.9.6"
  }
}